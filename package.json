{
  "name": "dylanbonneau",
  "version": "18.0.0",
  "scripts": {
    "ng": "ng",
    "serve:ssr:dylanBonneau": "node dist/dylanbonneau/server/server.mjs",
    "start": "ng serve",
    "start:ci": "ng serve --configuration production",
    "build": "ng build",
    "build:ci": "ng build --configuration production --optimization",
    "watch": "ng build --watch --configuration development",
    "watch:ci": "ng build --watch --configuration production",
    "test": "ng test",
    "test:ci": "ng test --browsers ChromeHeadlessCI --code-coverage --watch=false",
    "test:chrome": "ng test --browsers Chrome --code-coverage --watch=true",
    "lint": "npm run lint:styles && ng lint --type-check",
    "lint:styles": "stylelint \"src/app/**/*.scss\" \"src/assets/styles/**/*.scss\" --fix",
    "lint:ci": "ng lint",
    "lint:fix": "eslint -c .eslintrc.json --ext .ts src/ --fix",
    "commit": "git-cz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/platform-server": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@angular/ssr": "^18.2.14",
    "@commitlint/cli": "^18.6.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "cz-customizable": "^6.3.0",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "stylelint": "^16.2.1",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-scss": "^6.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.14",
    "@angular-eslint/builder": "^18.4.3",
    "@angular-eslint/eslint-plugin": "^18.4.3",
    "@angular-eslint/eslint-plugin-template": "^18.4.3",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "^18.4.3",
    "@angular/cli": "^18.2.14",
    "@angular/compiler-cli": "^18.2.13",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-complex-logic": "^0.3.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-max-return-statements-per-function": "^0.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-unicorn": "^53.0.0",
    "highlight.js": "^11.9.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-highlightjs": "^10.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
