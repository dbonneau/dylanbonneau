{
  "name": "dylanbonneau",
  "version": "17.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ci": "ng serve --configuration production",
    "build": "ng build",
    "build:ci": "ng build --configuration production --build-optimizer --optimization",
    "watch": "ng build --watch --configuration development",
    "watch:ci": "ng build --watch --configuration production",
    "test": "ng test",
    "test:ci": "ng test --browsers ChromeHeadlessCI --code-coverage --watch=false",
    "test:chrome": "ng test --browsers Chrome --code-coverage --watch=true",
    "lint": "npm run lint:styles && ng lint --type-check",
    "lint:styles": "stylelint \"src/app/**/*.scss\" \"src/assets/styles/**/*.scss\" --fix",
    "lint:ci": "ng lint",
    "lint:fix": "eslint -c .eslintrc.json --ext .ts src/ --fix",
    "commit": "git-cz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.2",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "@commitlint/cli": "^11.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "commitizen": "^4.2.2",
    "cz-customizable": "^6.3.0",
    "rxjs": "^7.8.1",
    "stylelint": "^13.13.1",
    "stylelint-config-property-sort-order-smacss": "^7.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-no-unsupported-browser-features": "^5.0.1",
    "stylelint-scss": "^3.20.1",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "^17.1.2",
    "@angular/compiler-cli": "^17.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "highlight.js": "^11.9.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ngx-highlightjs": "^10.0.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}